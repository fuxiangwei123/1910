.main{
    position: relative;
}
.list1 .aside{
    height: 620px;
    width: 212px;
}
.list1 .aside h3{
    line-height: 33px;
    background-color: #ffa800;
    text-align: center;
    height: 33px;
    font-size: 14px;
    font-weight: bold;
    color: #FFF;
}
.list1 .aside .hot-list{
    padding-top: 15px;
    border-left-width: 1px;
    border-left-style: solid;
    border-left-color: #ececec;
    padding-left: 10px;
    padding-right: 10px;
    padding-bottom: 0px;
    margin-top: 15px;
}
.list1 .aside .hot-list p{
    height: 32px;
}
.list1 .aside .hot-list p em{
    float: left;
    height: 15px;
    width: 17px;
    background: url(../images/em.jpg) no-repeat;
    color: #fff;
    text-align: center;
}
.list1 .aside .hot-list p a{
    float: left;
    padding: 0 4px;
    height: 15px;
    font: 12px/15px "";
}
.list1 .aside .hot-list .li-b{
    padding-bottom: 15px;
}
.list1 .aside .hot-list .li-b img{
    width: 48px;
    height: 62px;
    display: block;
    float: left;
    margin-right: 8px;
}
.list1 .aside .hot-list .li-br span{
    height: 20px;
    display: block;
    font: 12px/20px "";
    padding: 0 4px;
}
.intro{
    padding: 15px 0 0 10px;
}
.intro-t{
    margin-bottom: 10px;
}
.intro-t .fangda{
    width: 300px;
}
.intro-t .fangda .photo{
    padding: 10px;
    position: relative;
    /* overflow: hidden; */
    padding-left: 20px;
    position: relative;
    /* height: 306px; */
    /* height: 276px; */
}
.intro-t .fangda .photo-l{
    width: 250px;
    position: relative;
}
.intro-t .fangda .photo-l .kong{
    height: 160px;
    width: 160px;
    position: absolute;
    display: none;
    top: 0;
    left: 0;
    background:rgb(193, 180, 211);
    opacity: .6;
}
.intro-t .fangda .photo-l .ts{
    width: 250px;
    display: block;
    margin: 0 auto;
    height: 338px;
}
.intro-t .fangda .photo-r{
    width: 300px;
    height: 300px;
    position: absolute;
    display: none;
    overflow: hidden;
    top: 10px;
    left: 285px;
    border: 1px solid #000;
    background-color: #fff;
    background-repeat: no-repeat;
    background-position: center center;
    background-size: 443px 600px;
    z-index: 1000;
}
.intro-t .fangda .send{
    margin: 15px auto 0;
    text-align: center;
    width: 200px;
    height: 30px;
}
.intro-t .fangda .send img{
    height: 18px;
    width: 18px;
    cursor: pointer;
    text-align: center;
}
.intro-t .detail{
    padding-left: 20px;
    color: #000;
    width: 450px;
}
.intro-t .detail .d1{
    font: 12px/30px "";
    height: 30px;
    position: relative;
}
.intro-t .detail .d1 em{
    /* padding-left: 120px; */
    position: absolute;
    left: 200px;
    top: 0;
}
.intro-t .writer .d1 em{
    position: absolute;
    left: 200px;
    top: 0;
}
.intro-t .detail .writer.d1{
    position: relative;
}
.intro-t .detail #line{
    height: 15px;
    margin-top: 15px;
    border-top: 1px solid #ececec;
}
.intro-t .detail .name{
    font:  700 16px/35px "";
    color: #000;
    height: 35px;
    border-bottom: 1px solid #ececec;
}
.intro-t .detail .vipprice{
    height: 30px;
    font: 700 12px/30px "";
    color: #f00;
}
.intro-t .detail .mai{
    background-color: #f9f9f9;
    border: 1px solid #ececec;
    margin-top: 10px;
    padding: 15px;
}
.intro-t .detail .mai-t{
    height: 40px;
}
.txt{
    border: 1px solid #ececec;
    height: 22px;
    width: 42px;
    text-align: center;
}
.tu{
    height: 13px;
    width: 13px;
    cursor: pointer;
    vertical-align:middle;
}
.intro-t .detail .mai-b .m1{
    background-image: url(../images/buy_now.jpg);
    background-repeat: no-repeat;
    height: 36px;
    width: 132px;
    text-align: center;
    color: #fff;
    font: 18px/36px "";
    display: inline-block;
}
.intro-t .detail .mai-b .m2{
    background-image: url(../images/fovre_btn.jpg);
    background-repeat: no-repeat;
    height: 36px;
    width: 131px;
    display: inline-block;
    color: #fff;
    font: 18px/36px "";
    padding-left: 10px;
    margin-left: 10px;
}
.intro-b a{
    color: blueviolet;
}
.intro-b1{
    border-bottom: 1px solid #ececec;
    font: 12px/26px "";
    height: 26px;
}
.intro-b2{
    margin-top: 10px;
    height: 64px;
    background-color: #f9f9f9;
    font: 12px/64px "";
    padding-left: 26px;
}
