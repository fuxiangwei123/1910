<script src="js/floor.js"></script>
<script src="js/animate.js"></script>
<script>
        class List{
        constructor(options){
            this.cont = options.cont;
            this.url = options.url;
            this.sum1 = options.sum1;
            this.sum2 = options.sum2;
            this.snav = options.snav;
            this.width = options.tu.clientWidth;
            this.vip1 = options.vip1;
            this.vip2 = options.vip2;
            this.interval =100;
            this.idx = 0;
            this.load();
        }
        load(){
            var that = this;
            ajaxGet(that.url,function(res){
                // console.log(res);
                that.res =JSON.parse(res);
                // console.log(that.res);
                that.display();
            })
        }
        display(){
            var str = "";
            var str1 = "";
            var str2 = "";
            for(var i=this.sum1;i<this.res.length;i++){
                str+=`<div class="tu1 fl">
                            <a href="#">
                                <img src="${this.res[i].img}">
                                <p class="over">${this.res[i].name}</p>
                                <span>原价：￥${this.res[i].price}</span>
                                <span>现价：￥${this.res[i].vipprice}</span>
                            </a>
                        </div>
                `;
            }
            this.cont.innerHTML = str;
            this.addEvent();
            for(var j =3;j<12;j++){
                str1+=`<a href="#">
                    <img src="${this.res[j].img}">
                    <p>${this.res[j].name}</p>
                    <span>0积分</span>
                </a>
                `;
            }
            this.vip1.innerHTML = str1;
            for(var k=12;k<21;k++){
                str2+=`<a href="#">
                    <img src="${this.res[k].img}">
                    <p>${this.res[k].name}</p>
                    <span>0积分</span>
                </a>
                `;
            }
            this.vip2.innerHTML = str2;
        }
        addEvent(){
            var that = this;
            for(let i=0;i < this.snav.length;i++){
               that.snav[i].onmouseover = function(){
                    that.idx = i;
                    that.change();
               }
           }
        }
        change() {
            var that = this
            // console.log(this.cont);
            animate(this.cont, {left: -this.idx * this.width},this.interval,function() {
                    that.changeStyle();
            })
        }
        changeStyle(){
           
                // 循环所有小圆点
                for (var i = 0; i < this.snav.length; i++) {
                    // 比较 
                    if (i === this.idx) {
                        this.snav[i].className = "active";
                    } else {
                        this.snav[i].className = "";
                    }
                }
            
        }
    }
    new List({
        url:"http://localhost/xinhua/shop.json",
        cont:document.querySelector(".tuul"),
        snav:document.querySelectorAll(".snav"),
        tu:document.querySelector(".tushu"),
        vip1:document.querySelector(".v1"),
        vip2:document.querySelector(".v2"),
        sum1:0,
        sum2:9
    })


</script>
